{
  "name": "@pandoks.com/postgres",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:pgcat & pnpm run build:patroni & pnpm run build:pgbackrest",
    "build:pgcat": "docker build -f ./pgcat/Dockerfile ../../ -t localhost:12345/pgcat:latest",
    "build:patroni": "docker build -f ./patroni/Dockerfile ../../ -t localhost:12345/patroni:latest",
    "build:pgbackrest": "docker build -f ./pgbackrest/Dockerfile ../../ -t localhost:12345/pgbackrest:latest",
    "push": "pnpm run push:pgcat & pnpm run push:patroni & pnpm run push:pgbackrest",
    "push:pgcat": "docker push localhost:12345/pgcat:latest",
    "push:patroni": "docker push localhost:12345/patroni:latest",
    "push:pgbackrest": "docker push localhost:12345/pgbackrest:latest"
  }
}
