{
  "name": "@pandoks.com/valkey",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "gen": "pnpm run gen:deepcopy && pnpm run gen:crd",
    "controller-gen": "go run sigs.k8s.io/controller-tools/cmd/controller-gen@latest",
    "gen:deepcopy": "pnpm run controller-gen object paths=./api/...",
    "gen:crd": "pnpm run controller-gen crd:crdVersions=v1 paths=./api/... output:crd:artifacts:config=./kube",
    "build": "pnpm run build:valkey",
    "build:valkey": "docker build -f ./image/Dockerfile ../.. -t localhost:12345/valkey:latest",
    "push": "pnpm run push:valkey",
    "push:valkey": "docker push localhost:12345/valkey:latest"
  }
}
